
generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id       String @id @default(cuid())
  username String @unique
  password String
  name     String?
  image    String?
  @@map("user")

}
